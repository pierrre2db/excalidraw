# Excalidraw Environment Variables
# Copy this file to .env and fill in your values

# Development server port
VITE_APP_PORT=3000

# Google Drive Integration
# Get your CLIENT_ID from: https://console.cloud.google.com/
# Instructions in GOOGLE_DRIVE_SPEC.md
VITE_GOOGLE_DRIVE_CLIENT_ID=your_client_id_here.apps.googleusercontent.com

# Optional: Enable PWA in development
VITE_APP_ENABLE_PWA=false

# Optional: Disable live reload in development
# VITE_APP_DEV_DISABLE_LIVE_RELOAD=false

# Optional: Disable ESLint in Vite
# VITE_APP_ENABLE_ESLINT=true

# Optional: Git SHA for build version
# VITE_APP_GIT_SHA=

# Optional: Analytics tracking
# VITE_APP_ENABLE_TRACKING=false

# Optional: Sentry error tracking
# VITE_APP_DISABLE_SENTRY=true
